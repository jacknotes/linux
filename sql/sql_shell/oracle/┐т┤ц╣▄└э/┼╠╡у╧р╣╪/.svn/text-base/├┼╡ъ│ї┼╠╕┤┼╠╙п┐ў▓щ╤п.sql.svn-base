<BEFORERUN>
declare
vbegdate date;
venddate DATE;
vsort varchar2(200);
vgdcode varchar2(200);
vstorecode varchar2(200);
vBRAND  varchar2(200);
VFDIR   varchar2(200);
VSDIR   varchar2(200);
begin
  vbegdate := to_date('\(1,1)','yyyy.mm.dd');
  venddate := to_date('\(2,1)','yyyy.mm.dd');
  vsort := trim('\(3,1)')||'%';
  vgdcode := trim('\(4,1)')||'%';
  vstorecode :=trim('\(5,1)')||'%';  
  vBRAND :=trim('\(6,1)')||'%';  
  VFDIR := trim('\(7,1)')||'%';
  VSDIR := trim('\(8,1)')||'%';
 
  delete from H4TMP_CKDATA_TWICETIME;
  commit;
  INSERT INTO H4TMP_CKDATA_TWICETIME(FSEQ,FNOTE,FSTAT,FCKTIME,FACNTQTY,FQTY,SSEQ,SNOTE,SSTAT,SCKTIME,
  SACNTQTY,SQTY,RTLPRC,RTLBAL,CTOTALBAL,STCODE,STNAME,GDCODE,GDNAME,SCODE,SNAME,BCODE,BNAME)
  select aa.ckseq fseq,aa.note fnote,aa.stat fstat,aa.cktime fcktime,aa.acntqty facntqty,aa.qty fqty,
  bb.ckseq sseq,bb.note snote,bb.stat sstat,bb.cktime scktime,bb.acntqty sacntqty,bb.qty sqty,bb.rtlprc,bb.rtlbal,bb.ctotalbal,
  s.code stcode,s.name stname,g.code gdcode,g.name gdname ,sort.code scode,sort.name sname,brand.code bcode,brand.name bname from 
  (select a.ckseq,a.note,b.stat,b.cktime,b.store,b.gdgid,b.acntqty,b.qty 
  from ckdir a,ckdatas b where a.recheck = 0 and a.ckseq = b.ckseq) aa,
  (select a.ckseq,a.note,b.stat,b.cktime,b.store,b.gdgid,b.acntqty,b.qty ,b.rtlprc,b.rtlbal,b.camtbal+b.ctaxbal ctotalbal
  from ckdir a,ckdatas b where a.recheck = 1 and a.ckseq = b.ckseq) bb,
  store s,goodsh g,sort ,brand
  where aa.ckseq = substr(bb.note,instr(bb.note,'[',1,1)+1,instr(bb.note,']',1,1)-instr(bb.note,'[',1,1)-1)
  and aa.gdgid = bb.gdgid and aa.store = bb.store and aa.store = s.gid and aa.gdgid = g.gid AND BB.STAT = 3
  and g.sort = sort.code and g.brand = brand.code(+) 
      and (BB.CKTIME <= venddate or venddate is null)
      and (BB.CKTIME >= vbegdate or vbegdate is null)
      and (g.sort like vsort or vsort is null)
      and (g.code like vgdcode or vgdcode is null)
      and (S.code like vstorecode or vstorecode is null)
      and (G.BRAND like vBRAND or vBRAND is null)  
      AND (AA.CKSEQ LIKE VFDIR OR VFDIR IS NULL)
      AND (BB.CKSEQ LIKE VSDIR OR VSDIR IS NULL);
  COMMIT;
  
END;
</BEFORERUN>
