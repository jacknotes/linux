/************创建临时取仓库库存表***************/
PROMPT 创建总部及门店库存查看表;
DECLARE
  VRET    INT;
BEGIN
  VRET := HDDROPTABLE('H4RTMP_BUSINV');
  IF VRET = 0 then
    execute immediate 'create global temporary table H4RTMP_BUSINV (
      AREACODE         VARCHAR2(30),
      AREANAME         VARCHAR2(100),
      STOREGID         INT,
      STORECODE        VARCHAR2(30),
      STORENAME        VARCHAR2(100),
      WRHGID           INT,
      WRHCODE          VARCHAR2(30),
      WRHNAME          VARCHAR2(100),
      VDRGID           INT,
      VDRCODE          VARCHAR2(30),
      VDRNAME          VARCHAR2(100),
      BRANDCODE        VARCHAR2(15),
      BRANDNAME        VARCHAR2(50),
      GDGID            INT,
      GDCODE           VARCHAR2(30),
      GDCODE2          VARCHAR2(30),
      GDNAME           VARCHAR2(200),
      SLOT             VARCHAR2(20),
      QTY              NUMBER(24,4), 
      SUMQTY           NUMBER(24,4), 
      INVPRC           NUMBER(24,4), 
      AMT              NUMBER(24,4), 
      RTLPRC           NUMBER(24,4), 
      TOTAL            NUMBER(24,4), 
      ORDQTY           NUMBER(24,4), 
      DSPQTY           NUMBER(24,4), 
      ALCQTY           NUMBER(24,4), 
      BCKQTY           NUMBER(24,4),   
      RSVQTY           NUMBER(24,4),  
      RSVALCQTY        NUMBER(24,4),  
      ISPUQPCSTR       VARCHAR2(20),  
      MINMUNIT         VARCHAR2(20), 
      XLCODE           VARCHAR2(10), 
      XLNAME           VARCHAR2(30), 
      ZLCODE           VARCHAR2(10), 
      ZLNAME           VARCHAR2(30), 
      DLCODE           VARCHAR2(10), 
      DLNAME           VARCHAR2(30), 
      LIFECYCLE        VARCHAR2(50), 
      HIGHINV          NUMBER(24,4),   
      LOWINV           NUMBER(24,4),   
      OLD_CODE         VARCHAR2(30)
      ) on commit preserve rows';
  END IF;
END;
/
exec HDCREATESYNONYM('H4RTMP_BUSINV');
exec granttoqryrole('H4RTMP_BUSINV');

create or replace view v_businvs as
select 
GDGID,STORE,WRH,QTY,ORDQTY,DSPQTY,TOTAL,ALCQTY,BCKQTY,RSVQTY,RSV
ALCQTY
FROM BUSINVS
UNION ALL
select GID GDGID,1000000 STORE,WRH,0 QTY,0 ORDQTY,0 DSPQTY,0 
TOTAL,0 ALCQTY,0 BCKQTY,0 RSVQTY,0 RSVALCQTY
from goods
where (gid,wrh) not in (select gdgid,wrh from businvs where 
store = 1000000);
exec HDCREATESYNONYM('v_businvs');
exec granttoqryrole('v_businvs');
</REMARK>
<BEFORERUN>
DECLARE
V_FLAG           INT;
V_STORECODE      VARCHAR2(30);
V_GDCODE         VARCHAR2(30);
V_GDCODE2        VARCHAR2(30);
V_GDNAME         VARCHAR2(200);
V_WRHCODE        VARCHAR2(20);
V_VDRCODE        VARCHAR2(20);
V_SORTCODE       VARCHAR2(20);

BEGIN
  V_STORECODE  := TRIM('\(1,1)');
  V_GDCODE     := TRIM('\(2,1)');   
  V_GDCODE2    := TRIM('\(3,1)');
  V_GDNAME     := '%'||TRIM('\(4,1)')||'%';
  V_WRHCODE    := TRIM('\(5,1)');
  V_VDRCODE    := TRIM('\(6,1)');
  V_SORTCODE   := '%'||TRIM('\(7,1)')||'%';
  

  DELETE H4RTMP_BUSINV;
  SELECT CONVLOGINVTYPE INTO V_FLAG FROM FASYSTEM ; 
  IF V_FLAG = 0 THEN  -- 不记录门店库存
      -- 写入非实时移动加权时总部库存
      INSERT INTO H4RTMP_BUSINV(AREACODE, AREANAME, STOREGID, STORECODE, STORENAME, WRHGID, WRHCODE, WRHNAME,
        VDRGID, VDRCODE, VDRNAME, BRANDCODE, BRANDNAME, 
        GDGID, GDCODE, GDCODE2, GDNAME, SLOT, QTY, SUMQTY, 
        INVPRC, AMT, 
        RTLPRC, 
        TOTAL, 
        ORDQTY, DSPQTY, ALCQTY , BCKQTY, RSVQTY, RSVALCQTY, 
        ISPUQPCSTR, MINMUNIT, XLCODE, XLNAME, ZLCODE, ZLNAME, 
        DLCODE, DLNAME, LIFECYCLE, HIGHINV, LOWINV)      
      SELECT DISTINCT A.CODE AREACODE, A.NAME AREANAME, STORE.GID, STORE.CODE STORECODE, STORE.NAME STORENAME, WAREHOUSE.GID, DECODE(WAREHOUSE.GID,1000000,WAREHOUSE.CODE,'-') WRHCODE, DECODE(WAREHOUSE.GID,1000000,WAREHOUSE.NAME,'未知') WRHNAME,
        VENDORH.GID, VENDORH.CODE VDRCODE, VENDORH.NAME VDRNAME, BRAND.CODE BRANDCODE, BRAND.NAME BRANDNAME,
        GOODS.GID, GOODS.CODE GDCODE, GOODS.CODE2, GOODS.NAME GDNAME, GOODS.SLOT, BUSINVS.QTY QTY, ACTINVS.QTY SUMQTY, 
        ACTINVS.INVPRC INVPRC, ACTINVS.AMT+ACTINVS.TAX AMT,
        R.RTLPRC RTLPRC,
        ACTINVS.QTY*R.RTLPRC TOTAL,
        -- GETRTLPRC(STORE.GID, GOODS.GID) RTLPRC,
        -- ACTINVS.QTY*GETRTLPRC(STORE.GID, GOODS.GID) TOTAL,
        /*ROUND(DECODE((BUSINVS.QTY + BUSINVS.DSPQTY + BUSINVS.ALCQTY + BUSINVS.BCKQTY + BUSINVS.RSVQTY + BUSINVS.RSVALCQTY),0,0,BUSINVS.TOTAL/(BUSINVS.QTY + BUSINVS.DSPQTY + BUSINVS.ALCQTY + BUSINVS.BCKQTY + BUSINVS.RSVQTY + BUSINVS.RSVALCQTY)),4) RTLPRC, 
        BUSINVS.TOTAL TOTAL,*/ 
        BUSINVS.ORDQTY, BUSINVS.DSPQTY, BUSINVS.ALCQTY, BUSINVS.BCKQTY, BUSINVS.RSVQTY, BUSINVS.RSVALCQTY,
        GDQPC.QPCSTR ISPUQPCSTR, GOODS.MUNIT MINMUNIT, GSORT.CSCODE XLCODE, GSORT.CSNAME XLNAME, GSORT.BSCODE ZLCODE, GSORT.BSNAME ZLNAME,
        GSORT.ASCODE DLCODE, GSORT.ASNAME DLNAME, GOODS.LIFECYCLE, GOODS.HIGHINV, GOODS.LOWINV             
      FROM GOODSH GOODS, WAREHOUSE WAREHOUSE, STORE STORE, V_BUSINVS BUSINVS, ACTINVS ACTINVS, VENDORH VENDORH, GDQPC GDQPC,H4V_GOODSSORT GSORT,BRAND BRAND, AREA A, RPGGD R  
      WHERE  ACTINVS.GDGID = GOODS.GID 
         AND BUSINVS.GDGID = GOODS.GID
         AND STORE.GID = BUSINVS.STORE
         AND ACTINVS.WRH = WAREHOUSE.GID
         AND BUSINVS.STORE = 1000000
         AND BUSINVS.WRH = WAREHOUSE.GID
         AND STORE.GID = ACTINVS.STORE
         --总部取goods表供应商
         AND VENDORH.GID = GOODS.VDRGID
         AND GOODS.GID = GDQPC.GID
         AND GOODS.GID = R.GDGID
         AND GOODS.CODE = R.INPUTCODE
         AND STORE.GRPCODE = R.GCODE
         AND GDQPC.ISPU = 2
         AND GOODS.SORT = GSORT.DSCODE
         AND GOODS.GID = GSORT.GID
         AND NVL(GOODS.BRAND,'-') = BRAND.CODE
         AND STORE.AREA = A.CODE
         AND (STORE.CODE = V_STORECODE OR V_STORECODE IS NULL)
         AND (GOODS.CODE = V_GDCODE OR V_GDCODE IS NULL)
         AND (GOODS.CODE2 = V_GDCODE2 OR V_GDCODE2 IS NULL)
         AND (GOODS.NAME LIKE V_GDNAME OR V_GDNAME IS NULL)
         AND (WAREHOUSE.CODE = V_WRHCODE OR V_WRHCODE IS NULL)
         AND (VENDORH.CODE = V_VDRCODE OR V_VDRCODE IS NULL)
         AND (GOODS.SORT LIKE V_SORTCODE OR V_SORTCODE IS NULL);
     
    -- 写入非实时移动加权时门店库存 :商品在配送方案
    INSERT INTO H4RTMP_BUSINV(AREACODE, AREANAME, STOREGID, STORECODE, STORENAME, WRHGID, WRHCODE, WRHNAME, 
      VDRGID, VDRCODE, VDRNAME, BRANDCODE, BRANDNAME, 
      GDGID, GDCODE, GDCODE2, GDNAME, SLOT, QTY, SUMQTY, 
      INVPRC, AMT, 
      RTLPRC, TOTAL, ORDQTY, DSPQTY, ALCQTY , BCKQTY, RSVQTY, RSVALCQTY, 
      ISPUQPCSTR, MINMUNIT, XLCODE, XLNAME, ZLCODE, ZLNAME, 
      DLCODE, DLNAME, LIFECYCLE, HIGHINV, LOWINV)
    SELECT A.CODE AREACODE, A.NAME AREANAME, STORE.GID, STORE.CODE STORECODE, STORE.NAME STORENAME, WAREHOUSE.GID,DECODE(WAREHOUSE.GID,1000000,WAREHOUSE.CODE,'-') WRHCODE, DECODE(WAREHOUSE.GID,1000000,WAREHOUSE.NAME,'未知') WRHNAME,
      VENDORH.GID, VENDORH.CODE VDRCODE, VENDORH.NAME VDRNAME, BRAND.CODE BRANDCODE, BRAND.NAME BRANDNAME,
      GOODS.GID, GOODS.CODE GDCODE,GOODS.CODE2,  GOODS.NAME GDNAME, GOODS.SLOT, BUSINVS.QTY QTY, BUSINVS.QTY + BUSINVS.DSPQTY + BUSINVS.ALCQTY + BUSINVS.BCKQTY + BUSINVS.RSVQTY + BUSINVS.RSVALCQTY SUMQTY, 
      GOODS.CNTINPRC, GOODS.CNTINPRC*(BUSINVS.QTY + BUSINVS.DSPQTY + BUSINVS.ALCQTY + BUSINVS.BCKQTY + BUSINVS.RSVQTY + BUSINVS.RSVALCQTY) AMT,
      /*ROUND(DECODE(BUSINVS.QTY,0,0,BUSINVS.TOTAL/BUSINVS.QTY),4) RTLPRC, BUSINVS.TOTAL TOTAL, */
      --GETRTLPRC(STORE.GID, GOODS.GID) RTLPRC, BUSINVS.QTY*GETRTLPRC(STORE.GID, GOODS.GID) TOTAL, 
      R.RTLPRC RTLPRC, (BUSINVS.QTY + BUSINVS.DSPQTY + BUSINVS.ALCQTY + BUSINVS.BCKQTY + BUSINVS.RSVQTY + BUSINVS.RSVALCQTY)* R.RTLPRC TOTAL,     
      BUSINVS.ORDQTY, BUSINVS.DSPQTY, BUSINVS.ALCQTY, BUSINVS.BCKQTY, BUSINVS.RSVQTY, BUSINVS.RSVALCQTY,
      GDQPC.QPCSTR ISPUQPCSTR, GOODS.MUNIT MINMUNIT, GSORT.CSCODE XLCODE, GSORT.CSNAME XLNAME, GSORT.BSCODE ZLCODE, GSORT.BSNAME ZLNAME,
      GSORT.ASCODE DLCODE, GSORT.ASNAME DLNAME, GOODS.LIFECYCLE, GOODS.HIGHINV, GOODS.LOWINV
    FROM GOODSH GOODS, WAREHOUSE WAREHOUSE, STORE STORE, V_BUSINVS BUSINVS, VENDORH VENDORH, GDQPC GDQPC,H4V_GOODSSORT GSORT,BRAND BRAND, AREA A, ALCSCHEMEDTL ALCSCHEMEDTL,RPGGD R 
    WHERE BUSINVS.GDGID = GOODS.GID
       AND  BUSINVS.STORE = STORE.GID
       AND  BUSINVS.WRH = WAREHOUSE.GID
       AND  BUSINVS.WRH = 1
       AND  BUSINVS.STORE <> 1000000
       AND  STORE.ALCSCM = ALCSCHEMEDTL.CODE
       AND  VENDORH.GID = ALCSCHEMEDTL.VDRGID
       AND  GOODS.GID = ALCSCHEMEDTL.GDGID
       AND  GOODS.GID = GDQPC.GID
       AND GOODS.GID = R.GDGID
       AND GOODS.CODE = R.INPUTCODE
       AND STORE.GRPCODE = R.GCODE
       AND  GDQPC.ISPU = 2
       AND  GOODS.GID = GSORT.GID
       AND  GOODS.SORT = GSORT.DSCODE
       AND  NVL(GOODS.BRAND,'-') = BRAND.CODE
       AND  STORE.AREA = A.CODE
       AND (STORE.CODE = V_STORECODE OR V_STORECODE IS NULL)
       AND (GOODS.CODE = V_GDCODE OR V_GDCODE IS NULL)
       AND (GOODS.CODE2 = V_GDCODE2 OR V_GDCODE2 IS NULL)
       AND (GOODS.NAME LIKE V_GDNAME OR V_GDNAME IS NULL)
       AND (WAREHOUSE.CODE = V_WRHCODE OR V_WRHCODE IS NULL)
       AND (VENDORH.CODE = V_VDRCODE OR V_VDRCODE IS NULL)
       AND (GOODS.SORT LIKE V_SORTCODE OR V_SORTCODE IS NULL);       

    -- 写入非实时移动加权时门店库存 :商品有遗漏的
    INSERT INTO H4RTMP_BUSINV(AREACODE, AREANAME, STOREGID, STORECODE, STORENAME, WRHGID, WRHCODE, WRHNAME, 
      VDRGID, VDRCODE, VDRNAME, BRANDCODE, BRANDNAME, 
      GDGID, GDCODE, GDCODE2, GDNAME, SLOT, QTY, SUMQTY, 
      INVPRC, AMT, 
      RTLPRC, TOTAL, ORDQTY, DSPQTY, ALCQTY , BCKQTY, RSVQTY, RSVALCQTY, 
      ISPUQPCSTR, MINMUNIT, XLCODE, XLNAME, ZLCODE, ZLNAME, 
      DLCODE, DLNAME, LIFECYCLE, HIGHINV, LOWINV)
    SELECT A.CODE AREACODE, A.NAME AREANAME, STORE.GID, STORE.CODE STORECODE, STORE.NAME STORENAME, WAREHOUSE.GID,DECODE(WAREHOUSE.GID,1000000,WAREHOUSE.CODE,'-') WRHCODE, DECODE(WAREHOUSE.GID,1000000,WAREHOUSE.NAME,'未知') WRHNAME,
      VENDORH.GID, VENDORH.CODE VDRCODE, VENDORH.NAME VDRNAME, BRAND.CODE BRANDCODE, BRAND.NAME BRANDNAME,
      GOODS.GID, GOODS.CODE GDCODE,GOODS.CODE2,  GOODS.NAME GDNAME, GOODS.SLOT, BUSINVS.QTY QTY, BUSINVS.QTY + BUSINVS.DSPQTY + BUSINVS.ALCQTY + BUSINVS.BCKQTY + BUSINVS.RSVQTY + BUSINVS.RSVALCQTY SUMQTY, 
      GOODS.CNTINPRC, GOODS.CNTINPRC*(BUSINVS.QTY + BUSINVS.DSPQTY + BUSINVS.ALCQTY + BUSINVS.BCKQTY + BUSINVS.RSVQTY + BUSINVS.RSVALCQTY) AMT,
      /*ROUND(DECODE(BUSINVS.QTY,0,0,BUSINVS.TOTAL/BUSINVS.QTY),4) RTLPRC, BUSINVS.TOTAL TOTAL, */
      --GETRTLPRC(STORE.GID, GOODS.GID) RTLPRC, BUSINVS.QTY*GETRTLPRC(STORE.GID, GOODS.GID) TOTAL, 
      R.RTLPRC RTLPRC, (BUSINVS.QTY + BUSINVS.DSPQTY + BUSINVS.ALCQTY + BUSINVS.BCKQTY + BUSINVS.RSVQTY + BUSINVS.RSVALCQTY)* R.RTLPRC TOTAL,     
      BUSINVS.ORDQTY, BUSINVS.DSPQTY, BUSINVS.ALCQTY, BUSINVS.BCKQTY, BUSINVS.RSVQTY, BUSINVS.RSVALCQTY,
      GDQPC.QPCSTR ISPUQPCSTR, GOODS.MUNIT MINMUNIT, GSORT.CSCODE XLCODE, GSORT.CSNAME XLNAME, GSORT.BSCODE ZLCODE, GSORT.BSNAME ZLNAME,
      GSORT.ASCODE DLCODE, GSORT.ASNAME DLNAME, GOODS.LIFECYCLE, GOODS.HIGHINV, GOODS.LOWINV
    FROM GOODSH GOODS, WAREHOUSE WAREHOUSE, STORE STORE, V_BUSINVS BUSINVS, VENDORH VENDORH, GDQPC GDQPC,H4V_GOODSSORT GSORT,BRAND BRAND, AREA A, RPGGD R 
    WHERE BUSINVS.GDGID = GOODS.GID
       AND  BUSINVS.STORE = STORE.GID
       AND  BUSINVS.WRH = WAREHOUSE.GID
       AND  BUSINVS.WRH = 1
       AND  BUSINVS.STORE <> 1000000
       AND  GOODS.VDRGID = VENDORH.GID
       AND  GOODS.GID = GDQPC.GID
       AND  GOODS.GID = R.GDGID
       AND  GOODS.CODE = R.INPUTCODE
       AND  STORE.GRPCODE = R.GCODE
       AND  GDQPC.ISPU = 2
       AND  GOODS.GID = GSORT.GID
       AND  GOODS.SORT = GSORT.DSCODE
       AND  NVL(GOODS.BRAND,'-') = BRAND.CODE
       AND  STORE.AREA = A.CODE
       AND  NOT EXISTS (SELECT 1 FROM H4RTMP_BUSINV A
            WHERE BUSINVS.STORE = A.STOREGID
            AND BUSINVS.GDGID = A.GDGID
            AND BUSINVS.WRH = A.WRHGID)
       AND (STORE.CODE = V_STORECODE OR V_STORECODE IS NULL)
       AND (GOODS.CODE = V_GDCODE OR V_GDCODE IS NULL)
       AND (GOODS.CODE2 = V_GDCODE2 OR V_GDCODE2 IS NULL)
       AND (GOODS.NAME LIKE V_GDNAME OR V_GDNAME IS NULL)
       AND (WAREHOUSE.CODE = V_WRHCODE OR V_WRHCODE IS NULL)
       AND (VENDORH.CODE = V_VDRCODE OR V_VDRCODE IS NULL)
       AND (GOODS.SORT LIKE V_SORTCODE OR V_SORTCODE IS NULL);     
       
       
