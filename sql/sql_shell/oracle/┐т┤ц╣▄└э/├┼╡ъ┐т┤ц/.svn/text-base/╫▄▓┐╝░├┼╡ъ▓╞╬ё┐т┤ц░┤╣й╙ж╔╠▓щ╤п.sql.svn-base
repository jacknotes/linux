         AND ACTINVS.WRH = WAREHOUSE.GID
         AND BUSINVS.WRH = WAREHOUSE.GID
         AND STORE.GID = ACTINVS.STORE
         AND STORE.ALCSCM = ALCSCHEMEDTL.CODE
         AND VENDORH.GID = ALCSCHEMEDTL.VDRGID
         AND GOODS.GID = ALCSCHEMEDTL.GDGID
         AND GOODS.GID = GDQPC.GID
         AND GOODS.GID = R.GDGID
         AND GOODS.CODE = R.INPUTCODE
         AND STORE.GRPCODE = R.GCODE         
         AND GOODS.SORT = GSORT.DSCODE
         AND GOODS.GID = GSORT.GID
         AND NVL(GOODS.BRAND,'-') = BRAND.CODE
         AND STORE.AREA = A.CODE
         AND GDQPC.ISPU = 2
         AND ACTINVS.STORE <> 1000000
         AND (STORE.CODE = V_STORECODE OR V_STORECODE IS NULL)
         AND (GOODS.CODE = V_GDCODE OR V_GDCODE IS NULL)
         AND (GOODS.CODE2 = V_GDCODE2 OR V_GDCODE2 IS NULL)
         AND (GOODS.NAME LIKE V_GDNAME OR V_GDNAME IS NULL)
         AND (WAREHOUSE.CODE = V_WRHCODE OR V_WRHCODE IS NULL)
         AND (VENDORH.CODE = V_VDRCODE OR V_VDRCODE IS NULL)
         AND (GOODS.SORT LIKE V_SORTCODE OR V_SORTCODE IS NULL);             

     -- 门店库存(业务库存和财务库存都有数据):商品不在配送方案
      INSERT INTO H4RTMP_BUSINV(AREACODE, AREANAME, STOREGID, STORECODE, STORENAME, WRHGID, WRHCODE, WRHNAME,
        VDRGID, VDRCODE, VDRNAME, BRANDCODE, BRANDNAME, 
