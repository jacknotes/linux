        GSORT.ASCODE DLCODE, GSORT.ASNAME DLNAME, GOODS.LIFECYCLE, GOODS.HIGHINV, GOODS.LOWINV             
      FROM GOODSH GOODS, WAREHOUSE WAREHOUSE, STORE STORE, V_BUSINVS BUSINVS, ACTINVS ACTINVS, VENDORH VENDORH, GDQPC GDQPC,H4V_GOODSSORT GSORT,BRAND BRAND, AREA A, RPGGD R 
      WHERE  ACTINVS.GDGID = GOODS.GID 
         AND BUSINVS.GDGID = GOODS.GID
         AND STORE.GID = BUSINVS.STORE
         AND ACTINVS.WRH = WAREHOUSE.GID
         AND BUSINVS.WRH = WAREHOUSE.GID
         AND STORE.GID = ACTINVS.STORE
         AND VENDORH.GID = GOODS.VDRGID
         AND GOODS.GID = GDQPC.GID
         AND GOODS.GID = R.GDGID
         AND GOODS.CODE = R.INPUTCODE
         AND STORE.GRPCODE = R.GCODE         
         AND GOODS.SORT = GSORT.DSCODE
         AND GOODS.GID = GSORT.GID
         AND NVL(GOODS.BRAND,'-') = BRAND.CODE
         AND STORE.AREA = A.CODE
         AND GDQPC.ISPU = 2
         AND ACTINVS.STORE = 1000000
         AND (STORE.CODE = V_STORECODE OR V_STORECODE IS NULL)
         AND (GOODS.CODE = V_GDCODE OR V_GDCODE IS NULL)
         AND (GOODS.CODE2 = V_GDCODE2 OR V_GDCODE2 IS NULL)
         AND (GOODS.NAME LIKE V_GDNAME OR V_GDNAME IS NULL)
         AND (WAREHOUSE.CODE = V_WRHCODE OR V_WRHCODE IS NULL)
         AND (VENDORH.CODE = V_VDRCODE OR V_VDRCODE IS NULL)
         AND (GOODS.SORT LIKE V_SORTCODE OR V_SORTCODE IS NULL);             

     -- 门店库存(业务库存和财务库存都有数据):商品在配送方案
      INSERT INTO H4RTMP_BUSINV(AREACODE, AREANAME, STOREGID, STORECODE, STORENAME, WRHGID, WRHCODE, WRHNAME,
        VDRGID, VDRCODE, VDRNAME, BRANDCODE, BRANDNAME, 
        GDGID, GDCODE, GDCODE2, GDNAME, SLOT, QTY, SUMQTY, 
        INVPRC, AMT, 
        RTLPRC, 
        TOTAL, 
        ORDQTY, DSPQTY, ALCQTY , BCKQTY, RSVQTY, RSVALCQTY, 
        ISPUQPCSTR, MINMUNIT, XLCODE, XLNAME, ZLCODE, ZLNAME, 
        DLCODE, DLNAME, LIFECYCLE, HIGHINV, LOWINV)      
      SELECT DISTINCT A.CODE AREACODE, A.NAME AREANAME, STORE.GID, STORE.CODE STORECODE, STORE.NAME STORENAME, WAREHOUSE.GID, DECODE(WAREHOUSE.GID,1000000,WAREHOUSE.CODE,'-') WRHCODE, DECODE(WAREHOUSE.GID,1000000,WAREHOUSE.NAME,'未知') WRHNAME,
        VENDORH.GID, VENDORH.CODE VDRCODE, VENDORH.NAME VDRNAME, BRAND.CODE BRANDCODE, BRAND.NAME BRANDNAME,
        GOODS.GID, GOODS.CODE GDCODE, GOODS.CODE2, GOODS.NAME GDNAME, GOODS.SLOT, BUSINVS.QTY QTY, BUSINVS.QTY + BUSINVS.DSPQTY + BUSINVS.ALCQTY + BUSINVS.BCKQTY + BUSINVS.RSVQTY + BUSINVS.RSVALCQTY SUMQTY, 
        ACTINVS.INVPRC INVPRC, ACTINVS.AMT+ACTINVS.TAX AMT,
        R.RTLPRC RTLPRC,
        ACTINVS.QTY*R.RTLPRC TOTAL,
        --GETRTLPRC(STORE.GID, GOODS.GID) RTLPRC,
        /*ROUND(DECODE((BUSINVS.QTY + BUSINVS.DSPQTY + BUSINVS.ALCQTY + BUSINVS.BCKQTY + BUSINVS.RSVQTY + BUSINVS.RSVALCQTY),0,0,BUSINVS.TOTAL/(BUSINVS.QTY + BUSINVS.DSPQTY + BUSINVS.ALCQTY + BUSINVS.BCKQTY + BUSINVS.RSVQTY + BUSINVS.RSVALCQTY)),4) RTLPRC, */
        --ACTINVS.QTY * GETRTLPRC(STORE.GID, GOODS.GID) TOTAL, -- --库存售价额
        BUSINVS.ORDQTY, BUSINVS.DSPQTY, BUSINVS.ALCQTY, BUSINVS.BCKQTY, BUSINVS.RSVQTY, BUSINVS.RSVALCQTY,
        GDQPC.QPCSTR ISPUQPCSTR, GOODS.MUNIT MINMUNIT, GSORT.CSCODE XLCODE, GSORT.CSNAME XLNAME, GSORT.BSCODE ZLCODE, GSORT.BSNAME ZLNAME,
        GSORT.ASCODE DLCODE, GSORT.ASNAME DLNAME, GOODS.LIFECYCLE, GOODS.HIGHINV, GOODS.LOWINV              
      FROM GOODSH GOODS, WAREHOUSE WAREHOUSE, STORE STORE, V_BUSINVS BUSINVS, ACTINVS ACTINVS, ALCSCHEMEDTL ALCSCHEMEDTL,VENDORH VENDORH, GDQPC GDQPC,H4V_GOODSSORT GSORT,BRAND BRAND, AREA A, RPGGD R 
      WHERE  ACTINVS.GDGID = GOODS.GID 
         AND BUSINVS.GDGID = GOODS.GID
