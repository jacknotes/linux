        GDQPC.QPCSTR ISPUQPCSTR, GOODS.MUNIT MINMUNIT, GSORT.CSCODE XLCODE, GSORT.CSNAME XLNAME, GSORT.BSCODE ZLCODE, GSORT.BSNAME ZLNAME,
        GSORT.ASCODE DLCODE, GSORT.ASNAME DLNAME, GOODS.LIFECYCLE, GOODS.HIGHINV, GOODS.LOWINV              
      FROM GOODSH GOODS, WAREHOUSE WAREHOUSE, STORE STORE, V_BUSINVS BUSINVS, ACTINVS ACTINVS, VENDORH VENDORH, GDQPC GDQPC,H4V_GOODSSORT GSORT,BRAND BRAND, AREA A, RPGGD R 
      WHERE  ACTINVS.GDGID = GOODS.GID 
         AND BUSINVS.GDGID = GOODS.GID
         AND STORE.GID = BUSINVS.STORE
         AND ACTINVS.WRH = WAREHOUSE.GID
         AND BUSINVS.WRH = WAREHOUSE.GID
         AND STORE.GID = ACTINVS.STORE
         AND GOODS.VDRGID = VENDORH.GID
         AND GOODS.GID = GDQPC.GID
         AND GOODS.GID = R.GDGID
         AND GOODS.CODE = R.INPUTCODE
         AND STORE.GRPCODE = R.GCODE         
         AND GOODS.SORT = GSORT.DSCODE
         AND GOODS.GID = GSORT.GID
         AND NVL(GOODS.BRAND,'-') = BRAND.CODE
         AND STORE.AREA = A.CODE
       AND  NOT EXISTS (SELECT 1 FROM H4RTMP_BUSINV A
            WHERE BUSINVS.STORE = A.STOREGID
            AND BUSINVS.GDGID = A.GDGID
            AND BUSINVS.WRH = A.WRHGID)
         AND GDQPC.ISPU = 2
         AND ACTINVS.STORE <> 1000000
         AND (STORE.CODE = V_STORECODE OR V_STORECODE IS NULL)
