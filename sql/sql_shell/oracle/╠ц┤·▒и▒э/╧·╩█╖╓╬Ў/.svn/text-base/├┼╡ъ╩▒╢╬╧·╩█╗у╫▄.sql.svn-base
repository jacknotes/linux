<SQLQUERY>
<REMARK>
[标题]

[应用背景]

[结果列描述]

[必要的查询条件]

[实现方法]

[其它]
不含成本调整和成本差异
</REMARK>
<BEFORERUN>
declare
vdate date;
vbegdate date;
venddate date;
vareacode  varchar2(200);

begin
  vdate := to_date('\(1,1)','yyyy.mm.dd');
  vbegdate := to_date('\(2,1)','yyyy.mm.dd');
  venddate := to_date('\(3,1)','yyyy.mm.dd');
  vareacode := trim('\(4,1)')||'%';

delete from H4RTMP_SALEBYHOURS1;
delete from h4rtmp_salebyhourS;
--时段销售日报
insert into H4RTMP_SALEBYHOURS1(OCRDATE,FILDATE,STOREGID,
                 h00TRANS,h00RTOTAL,h01TRANS,h01RTOTAL,h02TRANS,h02RTOTAL,h03TRANS,h03RTOTAL,
                 h04TRANS,h04RTOTAL,h05TRANS,h05RTOTAL,h06TRANS,h06RTOTAL,h07TRANS,h07RTOTAL,
                 h08TRANS,h08RTOTAL,h09TRANS,h09RTOTAL,h10TRANS,h10RTOTAL,h11TRANS,h11RTOTAL,
                 h12TRANS,h12RTOTAL,h13TRANS,h13RTOTAL,h14TRANS,h14RTOTAL,h15TRANS,h15RTOTAL,
                 h16TRANS,h16RTOTAL,h17TRANS,h17RTOTAL,h18TRANS,h18RTOTAL,h19TRANS,h19RTOTAL,
                 h20TRANS,h20RTOTAL,h21TRANS,h21RTOTAL,h22TRANS,h22RTOTAL,h23TRANS,h23RTOTAL)
select OCRDATE,FILDATE,STOREGID,
                 h00TRANS,h00RTOTAL,h01TRANS,h01RTOTAL,h02TRANS,h02RTOTAL,h03TRANS,h03RTOTAL,
                 h04TRANS,h04RTOTAL,h05TRANS,h05RTOTAL,h06TRANS,h06RTOTAL,h07TRANS,h07RTOTAL,
                 h08TRANS,h08RTOTAL,h09TRANS,h09RTOTAL,h10TRANS,h10RTOTAL,h11TRANS,h11RTOTAL,
                 h12TRANS,h12RTOTAL,h13TRANS,h13RTOTAL,h14TRANS,h14RTOTAL,h15TRANS,h15RTOTAL,
                 h16TRANS,h16RTOTAL,h17TRANS,h17RTOTAL,h18TRANS,h18RTOTAL,h19TRANS,h19RTOTAL,
                 h20TRANS,h20RTOTAL,h21TRANS,h21RTOTAL,h22TRANS,h22RTOTAL,h23TRANS,h23RTOTAL
from RPT_salebyhourS r, store s
where r.storegid = s.gid
      and (r.fildate = vdate or vdate is null)
      and (r.fildate < venddate or venddate is null)
      and (r.fildate >= vbegdate or vbegdate is null)
      and (s.area like vareacode or vareacode is null);

--当天时段销售数据
    insert into h4rtmp_salebyhourS(OCRDATE,FILDATE,STOREGID,HOUR,TRANS,RTOTAL)
    select trunc(b1.rcvtime),trunc(b1.busdate),w.storegid,to_char(b1.FILDATE,'hh24'),
      COUNT(b1.FLOWNO),SUM(b1.realamt)
      from buy1s b1,workstation w,store s
     where b1.posno = w.no
      and w.storegid = s.gid
      and b1.rcvtime >= trunc(sysdate)
      and (trunc(b1.busdate) = vdate or vdate is null)
      and (b1.busdate < venddate or venddate is null)
      and (b1.busdate >= vbegdate or vbegdate is null)
      and (s.area like vareacode or vareacode is null)
     group by trunc(b1.rcvtime),trunc(b1.busdate),w.storegid,to_char(b1.FILDATE,'hh24');

    insert into H4RTMP_SALEBYHOURS1(OCRDATE,FILDATE,STOREGID,
                 h00TRANS,h00RTOTAL,h01TRANS,h01RTOTAL,h02TRANS,h02RTOTAL,h03TRANS,h03RTOTAL,
                 h04TRANS,h04RTOTAL,h05TRANS,h05RTOTAL,h06TRANS,h06RTOTAL,h07TRANS,h07RTOTAL,
                 h08TRANS,h08RTOTAL,h09TRANS,h09RTOTAL,h10TRANS,h10RTOTAL,h11TRANS,h11RTOTAL,
                 h12TRANS,h12RTOTAL,h13TRANS,h13RTOTAL,h14TRANS,h14RTOTAL,h15TRANS,h15RTOTAL,
                 h16TRANS,h16RTOTAL,h17TRANS,h17RTOTAL,h18TRANS,h18RTOTAL,h19TRANS,h19RTOTAL,
                 h20TRANS,h20RTOTAL,h21TRANS,h21RTOTAL,h22TRANS,h22RTOTAL,h23TRANS,h23RTOTAL)
    select OCRDATE,FILDATE,STOREGID,
           sum(decode(HOUR,'00',TRANS,0)),sum(decode(HOUR,'00',RTOTAL,0)),sum(decode(HOUR,'01',TRANS,0)),sum(decode(HOUR,'01',RTOTAL,0)),
           sum(decode(HOUR,'02',TRANS,0)),sum(decode(HOUR,'02',RTOTAL,0)),sum(decode(HOUR,'03',TRANS,0)),sum(decode(HOUR,'03',RTOTAL,0)),
           sum(decode(HOUR,'04',TRANS,0)),sum(decode(HOUR,'04',RTOTAL,0)),sum(decode(HOUR,'05',TRANS,0)),sum(decode(HOUR,'05',RTOTAL,0)),
           sum(decode(HOUR,'06',TRANS,0)),sum(decode(HOUR,'06',RTOTAL,0)),sum(decode(HOUR,'07',TRANS,0)),sum(decode(HOUR,'07',RTOTAL,0)),
           sum(decode(HOUR,'08',TRANS,0)),sum(decode(HOUR,'08',RTOTAL,0)),sum(decode(HOUR,'09',TRANS,0)),sum(decode(HOUR,'09',RTOTAL,0)),
           sum(decode(HOUR,'10',TRANS,0)),sum(decode(HOUR,'10',RTOTAL,0)),sum(decode(HOUR,'11',TRANS,0)),sum(decode(HOUR,'11',RTOTAL,0)),
           sum(decode(HOUR,'12',TRANS,0)),sum(decode(HOUR,'12',RTOTAL,0)),sum(decode(HOUR,'13',TRANS,0)),sum(decode(HOUR,'13',RTOTAL,0)),
           sum(decode(HOUR,'14',TRANS,0)),sum(decode(HOUR,'14',RTOTAL,0)),sum(decode(HOUR,'15',TRANS,0)),sum(decode(HOUR,'15',RTOTAL,0)),
           sum(decode(HOUR,'16',TRANS,0)),sum(decode(HOUR,'16',RTOTAL,0)),sum(decode(HOUR,'17',TRANS,0)),sum(decode(HOUR,'17',RTOTAL,0)),
           sum(decode(HOUR,'18',TRANS,0)),sum(decode(HOUR,'18',RTOTAL,0)),sum(decode(HOUR,'19',TRANS,0)),sum(decode(HOUR,'19',RTOTAL,0)),
           sum(decode(HOUR,'20',TRANS,0)),sum(decode(HOUR,'20',RTOTAL,0)),sum(decode(HOUR,'21',TRANS,0)),sum(decode(HOUR,'21',RTOTAL,0)),
           sum(decode(HOUR,'22',TRANS,0)),sum(decode(HOUR,'22',RTOTAL,0)),sum(decode(HOUR,'23',TRANS,0)),sum(decode(HOUR,'23',RTOTAL,0))
    from h4rtmp_salebyhourS
    group by  OCRDATE,FILDATE,STOREGID;
end;
</BEFORERUN>
