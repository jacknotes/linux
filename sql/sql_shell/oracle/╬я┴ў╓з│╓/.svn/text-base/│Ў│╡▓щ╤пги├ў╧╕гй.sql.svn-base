create global temporary table HDTMP_DISPATCH
(
NUM varchar2(20),
CLS varchar2(20),
FILDATE date,
DRIVER  varchar2(20),
CARCODE VARCHAR2(128),
CARLICS VARCHAR2(128),
STORECOUNT int,
STAT varchar2(20),
STORE VARCHAR2(100)
);
EXEC HDCREATESYNONYM('HDTMP_DISPATCH')
EXEC GRANTTOQRYROLE('HDTMP_DISPATCH')
</REMARK>
<BEFORERUN>
declare
begin
  delete from HDTMP_DISPATCH; 
  commit;
 insert into HDTMP_DISPATCH(NUM,CLS,FILDATE,DRIVER,CARCODE,CARLICS,STORECOUNT,STAT,STORE)
  select a.num 单据号,a.cls 单据类型,a.fildate 制单日期,c.name 驾驶员姓名,'' 车辆编码,a.carlics 车牌号,d.storecount 并车家数,
  decode(a.stat,0,'未审核',100,'已审核',300,'已完成') 单据状态,b.client 客户编码及名称
  from dispatch a, dispatchdtl b,employee c,
  (select num,cls,count(distinct(client)) storecount from dispatchdtl group by num ,cls) d
 where a.num = b.num and a.cls = b.cls
   and a.driver = c.gid
   and a.num = d.num and a.cls = d.cls
  and a.cls = '出车登记' 
  group by a.num ,a.cls,a.fildate ,c.name,a.carlics,d.storecount,
  decode(a.stat,0,'未审核',100,'已审核',300,'已完成'),b.client 
  order by a.num,b.client;
commit;
end;
</BEFORERUN>
