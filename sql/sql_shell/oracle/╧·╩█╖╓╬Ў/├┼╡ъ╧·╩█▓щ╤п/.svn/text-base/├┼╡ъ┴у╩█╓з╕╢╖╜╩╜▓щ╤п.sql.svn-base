<VERSION>4.1.1</VERSION>
<SQLQUERY>
<REMARK>
[标题]

[应用背景]

[结果列描述]

[必要的查询条件]

[实现方法]

[其它]
</REMARK>
<BEFORERUN>
declare

vbegdate date;
venddate date;
begin
  vbegdate := to_date('\(1,1)','yyyy.mm.dd');
  venddate := to_date('\(2,1)','yyyy.mm.dd');

delete from H4TMP_BUYCURRENCY;
commit;

insert into H4TMP_BUYCURRENCY(FLOWNO,EMPCODE,EMPNAME,POSNO,STCODE,STNAME,REALAMT,FAVAMOUNT,CURNAME1,AMOUNT1,
CURNAME2,AMOUNT2,CURNAME3,AMOUNT3,CURNAME4,AMOUNT4,CURNAME5,AMOUNT5,CURNAME6,AMOUNT6,
ELSENAME,ELSEAMOUNT,FILDATE,SALECLS,MEMO,BUSDATE)
SELECT BUY11S.FLOWNO FLOWNO, EMPLOYEEH.CODE EMPCODE, EMPLOYEEH.NAME EMPNAME, BUY11S.POSNO POSNO, 
STORE.CODE STCODE, STORE.NAME STNAME, BUY1S.REALAMT REALAMT, SUM(BUY11S.FAVAMOUNT) FAVAMOUNT, 
NULL CURNAME1 ,SUM(decode(BUY11S.CURRENCY,'-1',1,'1',1,0) * BUY11S.AMOUNT) AMOUNT1, 
NULL CURNAME2 ,SUM(decode(BUY11S.CURRENCY,'2',1,0) * BUY11S.AMOUNT) AMOUNT2, 
NULL CURNAME3 ,SUM(decode(BUY11S.CURRENCY,'3',1,0) * BUY11S.AMOUNT) AMOUNT3, 
NULL CURNAME4 ,SUM(decode(BUY11S.CURRENCY,'4',1,0) * BUY11S.AMOUNT) AMOUNT4,
NULL CURNAME5 ,SUM(decode(BUY11S.CURRENCY,'5',1,0) * BUY11S.AMOUNT) AMOUNT5, 
NULL CURNAME6 ,SUM(decode(BUY11S.CURRENCY,'6',1,0) * BUY11S.AMOUNT) AMOUNT6,
'其他' ELSENAME ,SUM((CASE WHEN BUY11S.CURRENCY>6 THEN 1 ELSE 0 END) * BUY11S.AMOUNT) ELSEAMOUNT, 
BUY1S.FILDATE FILDATE, (case when  BUY1S.REALAMT > 0 then '销售' else '销售退货' end) salecls, 
BUY1S.MEMO MEMO, BUY1S.BUSDATE BUSDATE
FROM BUY11S BUY11S, STORE STORE, WORKSTATION WORKSTATION, BUY1S BUY1S, EMPLOYEEH EMPLOYEEH
WHERE (BUY11S.CURRENCY <> -2)
 and  (BUY11S.POSNO = BUY1S.POSNO)
 and  (BUY11S.FLOWNO = BUY1S.FLOWNO)
 and  (BUY1S.POSNO = WORKSTATION.NO)
 and  (BUY1S.CASHIER = EMPLOYEEH.GID)
 and  (WORKSTATION.STOREGID = STORE.GID) 
 and (BUY1S.BUSDATE >= vbegdate or vbegdate is null) 
 and (BUY1S.BUSDATE < venddate OR venddate IS NULL)
GROUP BY BUY11S.FLOWNO, EMPLOYEEH.CODE, EMPLOYEEH.NAME, BUY11S.POSNO, STORE.CODE, STORE.NAME,
 BUY1S.REALAMT, BUY1S.FILDATE, (case when  BUY1S.REALAMT > 0 then '销售' else '销售退货' end), BUY1S.MEMO, BUY1S.BUSDATE
ORDER BY POSNO ASC, FLOWNO ASC;

update H4TMP_BUYCURRENCY set CURNAME1 = (select name from currency where code = '1');
update H4TMP_BUYCURRENCY set CURNAME2 = (select name from currency where code = '2');
update H4TMP_BUYCURRENCY set CURNAME3 = (select name from currency where code = '3');
update H4TMP_BUYCURRENCY set CURNAME4 = (select name from currency where code = '4');
update H4TMP_BUYCURRENCY set CURNAME5 = (select name from currency where code = '5');
update H4TMP_BUYCURRENCY set CURNAME6 = (select name from currency where code = '6');
update H4TMP_BUYCURRENCY set CURNAME1 = '无支付方式'  where CURNAME1 is null;
update H4TMP_BUYCURRENCY set CURNAME2 = '无支付方式'  where CURNAME2 is null;
update H4TMP_BUYCURRENCY set CURNAME3 = '无支付方式'  where CURNAME3 is null;
update H4TMP_BUYCURRENCY set CURNAME4 = '无支付方式'  where CURNAME4 is null;
update H4TMP_BUYCURRENCY set CURNAME5 = '无支付方式'  where CURNAME5 is null;
update H4TMP_BUYCURRENCY set CURNAME6 = '无支付方式'  where CURNAME6 is null;
commit;
end;
</BEFORERUN>
