<SQLQUERY>
<REMARK>
[标题]

[应用背景]

[结果列描述]

[必要的查询条件]

[实现方法]

[其它]
</REMARK>
<BEFORERUN>
DECLARE

VDATE  DATE;
VGDCODE VARCHAR2(50);
VWCODE  VARCHAR2(50);
VSORT   VARCHAR2(50);
VBRAND  VARCHAR2(50);
VVDRCODE  VARCHAR2(50);
VNSTARTDATE DATE;
VNFINISHDATE  DATE;
VLSTARTDATE   DATE;
VLFINISHDATE  DATE;
BEGIN
vdate := to_date('\(1,1)','yyyy.mm.dd');
VGDCODE := trim('\(2,1)');
VWCODE := trim('\(3,1)');
VSORT := trim('\(4,1)');
VBRAND := trim('\(5,1)');
VVDRCODE := trim('\(6,1)');
--取本年和去年的时间范围
BEGIN
SELECT M.STARTDATE INTO VNSTARTDATE FROM MONTHSETTLE M WHERE NO = TO_CHAR(VDATE,'YYYY')||'01';
exception
  when no_data_found then
      VNSTARTDATE := TRUNC(VDATE,'YYYY');
END;

BEGIN
SELECT M.FINISHDATE INTO VNFINISHDATE FROM MONTHSETTLE M WHERE NO = TO_CHAR(VDATE,'YYYY')||'12';
exception
  when no_data_found then
      VNFINISHDATE := add_months(trunc(VDATE,'YYYY'),12)-1;
END;

BEGIN
SELECT M.STARTDATE INTO VLSTARTDATE FROM MONTHSETTLE M WHERE NO = TO_CHAR(VDATE,'YYYY')-1||'01';
exception
  when no_data_found then
      VLSTARTDATE := add_months(trunc(VDATE,'YYYY'),-12);
END;

BEGIN
SELECT M.FINISHDATE INTO VLFINISHDATE FROM MONTHSETTLE M WHERE NO = TO_CHAR(VDATE,'YYYY')-1||'12';
exception
  when no_data_found then
      VLFINISHDATE := TRUNC(VDATE,'YYYY')-1;
END;

DELETE FROM HDTMP_RPT_HISINVIN;
COMMIT;
---插入当前库存记录
INSERT INTO HDTMP_RPT_HISINVIN(LSTDATE,WRHCODE,WRHNAME,GDCODE,GDNAME,VDRCODE,VDRNAME,SORT,SORTNAME,Brand,Brandname,QTY,ORDQTY)
SELECT VDATE,W.CODE,W.NAME,G.CODE,G.NAME,V.CODE,V.NAME,R.SCODE,R.SNAME,A.CODE,A.NAME,SUM(B.QTY+B.DSPQTY+B.ALCQTY+B.BCKQTY+B.RSVQTY+B.RSVALCQTY),SUM(ORDQTY)
FROM BUSINVS B,GOODSH G,WAREHOUSEH W,BRAND A,SORTNAME R,VENDORH V
where (G.CODE = VGDCODE OR VGDCODE IS NULL)
AND (W.CODE = VWCODE OR VWCODE IS NULL)
AND (G.SORT = VSORT OR VSORT IS NULL)
AND (G.BRAND = VBRAND OR VBRAND IS NULL)
AND (V.CODE = VVDRCODE OR VVDRCODE IS NULL)
AND G.BRAND = A.CODE(+)
AND G.SORT = R.SCODE
AND G.VDRGID = V.GID
AND R.ACODE = '0000'
AND B.WRH = W.GID
AND B.GDGID = G.GID
AND (B.QTY+B.DSPQTY+B.ALCQTY+B.BCKQTY+B.RSVQTY+B.RSVALCQTY+B.ORDQTY <>0)
AND b.store = 1000000
GROUP BY VDATE,W.CODE,W.NAME,G.CODE,G.NAME,V.CODE,V.NAME,R.SCODE,R.SNAME,A.CODE,A.NAME;
--插入当天进货数量
INSERT INTO HDTMP_RPT_HISINVIN(LSTDATE,WRHCODE,WRHNAME,GDCODE,GDNAME,VDRCODE,VDRNAME,SORT,SORTNAME,Brand,Brandname,NDAYSKTINQTY)
SELECT VDATE,W.CODE,W.NAME,G.CODE,G.NAME,V.CODE,V.NAME,R.SCODE,R.SNAME,A.CODE,A.NAME,SUM(D.QTY)
FROM DRPT D,GOODSH G,WAREHOUSEH W,BRAND A,SORTNAME R,VENDORH V
WHERE (G.CODE = VGDCODE OR VGDCODE IS NULL)
AND (W.CODE = VWCODE OR VWCODE IS NULL)
AND (G.SORT = VSORT OR VSORT IS NULL)
AND (G.BRAND = VBRAND OR VBRAND IS NULL)
AND (V.CODE = VVDRCODE OR VVDRCODE IS NULL)
AND G.BRAND = A.CODE(+)
AND G.SORT = R.SCODE
AND D.VDRGID = V.GID
AND R.ACODE = '0000'
AND D.SWRH = W.GID
AND D.GDGID = G.GID
AND D.cls in ('自营进')
AND D.FILDATE = VDATE
GROUP BY VDATE,W.CODE,W.NAME,G.CODE,G.NAME,V.CODE,V.NAME,R.SCODE,R.SNAME,A.CODE,A.NAME;
--插入当月收货数量
INSERT INTO HDTMP_RPT_HISINVIN(LSTDATE,WRHCODE,WRHNAME,GDCODE,GDNAME,VDRCODE,VDRNAME,SORT,SORTNAME,Brand,Brandname,NMONSTKINQTY)
SELECT VDATE,W.CODE,W.NAME,G.CODE,G.NAME,V.CODE,V.NAME,R.SCODE,R.SNAME,A.CODE,A.NAME,SUM(D.QTY)
FROM DRPT D,GOODSH G,WAREHOUSEH W,BRAND A,SORTNAME R,VENDORH V,MONTHSETTLE M
WHERE (G.CODE = VGDCODE OR VGDCODE IS NULL)
AND (W.CODE = VWCODE OR VWCODE IS NULL)
AND (G.SORT = VSORT OR VSORT IS NULL)
AND (G.BRAND = VBRAND OR VBRAND IS NULL)
AND (V.CODE = VVDRCODE OR VVDRCODE IS NULL)
AND G.BRAND = A.CODE(+)
AND G.SORT = R.SCODE
AND D.VDRGID = V.GID
AND R.ACODE = '0000'
AND D.SWRH = W.GID
AND D.GDGID = G.GID
AND D.cls in ('自营进')
AND D.FILDATE > m.startdate
AND D.FILDATE <= m.finishdate
AND VDATE > m.startdate 
AND VDATE <= m.finishdate 
GROUP BY VDATE,W.CODE,W.NAME,G.CODE,G.NAME,V.CODE,V.NAME,R.SCODE,R.SNAME,A.CODE,A.NAME;
--插入当年收货数量
INSERT INTO HDTMP_RPT_HISINVIN(LSTDATE,WRHCODE,WRHNAME,GDCODE,GDNAME,VDRCODE,VDRNAME,SORT,SORTNAME,Brand,Brandname,Nyearstkinqty)
SELECT VDATE,W.CODE,W.NAME,G.CODE,G.NAME,V.CODE,V.NAME,R.SCODE,R.SNAME,A.CODE,A.NAME,SUM(D.QTY)
FROM DRPT D,GOODSH G,WAREHOUSEH W,BRAND A,SORTNAME R,VENDORH V
WHERE (G.CODE = VGDCODE OR VGDCODE IS NULL)
AND (W.CODE = VWCODE OR VWCODE IS NULL)
AND (G.SORT = VSORT OR VSORT IS NULL)
AND (G.BRAND = VBRAND OR VBRAND IS NULL)
AND (V.CODE = VVDRCODE OR VVDRCODE IS NULL)
AND G.BRAND = A.CODE(+)
AND G.SORT = R.SCODE
AND D.VDRGID = V.GID
AND R.ACODE = '0000'
AND D.SWRH = W.GID
AND D.GDGID = G.GID
AND D.cls in ('自营进')
AND D.FILDATE > VNSTARTDATE
AND D.FILDATE <= VNFINISHDATE
GROUP BY VDATE,W.CODE,W.NAME,G.CODE,G.NAME,V.CODE,V.NAME,R.SCODE,R.SNAME,A.CODE,A.NAME;
--插入去年收货数量
INSERT INTO HDTMP_RPT_HISINVIN(LSTDATE,WRHCODE,WRHNAME,GDCODE,GDNAME,VDRCODE,VDRNAME,SORT,SORTNAME,Brand,Brandname,LYEARSTKINQTY)
SELECT VDATE,W.CODE,W.NAME,G.CODE,G.NAME,V.CODE,V.NAME,R.SCODE,R.SNAME,A.CODE,A.NAME,SUM(D.QTY)
FROM DRPT D,GOODSH G,WAREHOUSEH W,BRAND A,SORTNAME R,VENDORH V
WHERE (G.CODE = VGDCODE OR VGDCODE IS NULL)
AND (W.CODE = VWCODE OR VWCODE IS NULL)
AND (G.SORT = VSORT OR VSORT IS NULL)
AND (G.BRAND = VBRAND OR VBRAND IS NULL)
AND (V.CODE = VVDRCODE OR VVDRCODE IS NULL)
AND G.BRAND = A.CODE(+)
AND G.SORT = R.SCODE
AND D.VDRGID = V.GID
AND R.ACODE = '0000'
AND D.SWRH = W.GID
AND D.GDGID = G.GID
AND D.cls in ('自营进')
AND D.FILDATE > VLSTARTDATE
AND D.FILDATE <= VLFINISHDATE
GROUP BY VDATE,W.CODE,W.NAME,G.CODE,G.NAME,V.CODE,V.NAME,R.SCODE,R.SNAME,A.CODE,A.NAME;
commit;
END;
