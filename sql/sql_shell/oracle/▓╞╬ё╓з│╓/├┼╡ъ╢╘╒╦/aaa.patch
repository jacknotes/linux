+
+[必要的查询条件]
+
+[实现方法]
+create global temporary table H4RTMP_DOORPRICE
+(
+  JCODE     VARCHAR2(20),    
+  JNAME     VARCHAR2(40),    
+  JSTORE    VARCHAR2(20),    
+  JOCRDATE  DATE,              
+  JSRCCLS   VARCHAR2(20),   
+  JSRCNUM   VARCHAR2(20),  
+  JADDPRICE NUMBER(24,4), 
+  JENDPRICE NUMBER(24,4),
+  JPRICE    NUMBER(24,4),  
+  dpscode   varchar2(20),    
+  dpsname   varchar2(80), 
+  chcode    varchar2(20),    
+  chname    varchar2(80),    
+  scode    varchar2(20),
+  sname    varchar2(80),
+  stat        integer,
+  STNOTE  varchar2(100),
+  BNAME   varchar2(40),
+  cls         varchar2(20)
+)
+on commit preserve rows;
+BEGIN
+    HDCREATESYNONYM('H4RTMP_DOORPRICE');
+    GRANTTOQRYROLE2('H4RTMP_DOORPRICE');
+END; 
+----------------------------
+create global temporary table H4RTMP_STORSUBJ
+(    num    CHAR(14),
+     cls       VARCHAR2(10),
+     note     VARCHAR2(100),
+     name   VARCHAR2(40)
+)
+on commit preserve rows;
+BEGIN
+    HDCREATESYNONYM('H4RTMP_STORSUBJ');
+    GRANTTOQRYROLE2('H4RTMP_STORSUBJ');
+END; 
+-- Grant/Revoke object privileges 
+grant insert, update, delete on H4RTMP_DOORPRICE to ROLE_HDAPP;
+grant insert, update, delete on H4RTMP_DOORPRICE to ROLE_HDQRY;
+[其它]
+</REMARK>
+<BEFORERUN>
+declare 
+  vscode varchar(20);
+  vsname varchar(80);
+  vdcode varchar(20);
+  vccode varchar(20);
+  vdpsgid int;
+  vstcode varchar(20);
+  vstname varchar(80);
+  cursor i is 
+  select SNUM,SCLS,SNOTE,BNAME from H4RTMP_STORSUBJ;
+begin
+    vscode := '\(1,1)';
+    vsname := '\(2,1)';
+    vdcode := '\(3,1)';
+    vccode := '\(4,1)';
+    delete from H4RTMP_DOORPRICE;
+    commit;
+    delete from H4RTMP_STORSUBJ;
+    commit;
+  if vscode is not null then
+    select  sa.dpsgid,st.code,st.name into vdpsgid,vstcode,vstname from 
+storeaccount sa,store st where sa.stgid=st.gid and st.code=vscode;
+     insert into H4RTMP_DOORPRICE(jcode,jname,jstore,jocrdate,jsrccls,jsrcnum,
+jaddprice,jprice,dpscode,dpsname,chcode,chname,cls,scode,sname,stat)
+    select s.code,s.name,g.actcls,g.ocrdate,g.srccls,g.srcnum,nvl(g.alctotal,0),
+nvl(g.orialctotal,0) + nvl(g.alctotal,0),d.code,d.name,c.code,c.name,
+decode(g.srccls,'加盟金缴款','缴款','加盟金扣款','扣款'),
+           vstcode,vstname,g.srcstat
+     from storealcctrllog g,store s, dpsaccount d,chainh c
+    where g.store = s.gid and g.alctotal >=0 
+      and d.gid=g.dpsgid
+      and d.code like vdcode||'%' 
+      --and s.code like vscode||'%'
+      and d.chain = c.gid(+)
+      and d.gid =vdpsgid; 
+
+  insert into H4RTMP_DOORPRICE(jcode,jname,jstore,jocrdate,jsrccls,jsrcnum,
+jendprice,jprice,dpscode,dpsname,chcode,chname,cls,scode,sname,stat)
+    select s.code,s.name,g.actcls,g.ocrdate,g.srccls,g.srcnum,nvl(g.alctotal,0),
+nvl(g.orialctotal,0) + nvl(g.alctotal,0),d.code,d.name,c.code,c.name,
+decode(g.srccls,'加盟金缴款','缴款','加盟金扣款','扣款'),
+           vstcode,vstname,g.srcstat
+     from storealcctrllog g,store s, dpsaccount d,chainh c
+    where g.store = s.gid and g.alctotal <0 
+      and d.gid=g.dpsgid
+      and d.code like vdcode||'%'
+      and d.chain = c.gid(+) 
+      --and s.code like vscode||'%'
+      and d.gid = vdpsgid; 
+  else
+     insert into H4RTMP_DOORPRICE(jcode,jname,jstore,jocrdate,jsrccls,jsrcnum,
+jaddprice,jprice,dpscode,dpsname,chcode,chname,cls,stat)
+    select s.code,s.name,g.actcls,g.ocrdate,g.srccls,g.srcnum,nvl(g.alctotal,0),
+nvl(g.orialctotal,0) + nvl(g.alctotal,0),d.code,d.name,
+      c.code,c.name,decode(g.srccls,'加盟金缴款','缴款','加盟金扣款','扣款'),
+g.srcstat
+     from storealcctrllog g,store s, dpsaccount d,chainh c
+    where g.store = s.gid and g.alctotal >=0 
+      and d.gid=g.dpsgid
+      and d.chain = c.gid(+)
+      --and d.code like vdcode||'%'
+      and s.code like vscode||'%'
+; 
+
+  insert into H4RTMP_DOORPRICE(jcode,jname,jstore,jocrdate,jsrccls,jsrcnum,
+jendprice,jprice,dpscode,dpsname,chcode,chname,cls,stat)
+    select s.code,s.name,g.actcls,g.ocrdate,g.srccls,g.srcnum,nvl(g.alctotal,0),
+nvl(g.orialctotal,0) + nvl(g.alctotal,0),d.code,d.name,c.code,c.name,
+     decode(g.srccls,'加盟金缴款','缴款','加盟金扣款','扣款'),g.srcstat
+     from storealcctrllog g,store s, dpsaccount d,chainh c
+    where g.store = s.gid and g.alctotal <0 
+      and d.gid=g.dpsgid
+      and d.chain = c.gid(+)
+      and d.code like vdcode||'%'
+      --and s.code like vscode||'%'
+; 
+   end if;
+  commit;
+  
+  insert into H4RTMP_STORSUBJ(snum,scls,snote,Bname)
+    select s.num,s.cls,s.note,b.name from STOREALCPAY s,SUBJECTATT b where 
+s.scode = b.code;   commit;
+    FOR R IN I LOOP
+      UPDATE H4RTMP_DOORPRICE SET H4RTMP_DOORPRICE.Stnote = R.SNOTE,
+H4RTMP_DOORPRICE.Bname = R.BNAME WHERE H4RTMP_DOORPRICE.Jsrcnum = R.SNUM AND 
+H4RTMP_DOORPRICE.Cls = R.SCLS;
+    END LOOP;
+  COMMIT;
+delete from H4RTMP_DOORPRICE
+ where nvl(jaddprice, 0) = 0
+   and nvl(jendprice, 0) = 0;
+   commit ;
+---增加门店小计
+  insert into H4RTMP_DOORPRICE(jcode,jname,jstore,jocrdate,jsrccls,jsrcnum,
+jaddprice,jendprice,jprice,dpscode,dpsname,chcode,chname,cls,stat)
+select jcode,jname,'门店小计',max(jocrdate)+0.01,null,null,
+sum(jaddprice),sum(jendprice),null,dpscode,dpsname,chcode,chname,null,null
+from H4RTMP_DOORPRICE group by jcode,jname,dpscode,dpsname,chcode,chname;
+commit;
+end;
