             TRUNC(LOG.TIME) TIME,
             VFLAG
        FROM STKIN     D,
             STKINLOG LOG,
             VENDOR    VBILLTO,
             VENDOR    VVENDOR,
             MODULESTAT M
       WHERE D.NUM = LOG.NUM
         AND D.CLS = LOG.CLS
         AND D.CLS = '自营进'
         AND D.BILLTO = VBILLTO.GID
         AND D.VENDOR = VVENDOR.GID
         AND LOG.STAT in (1000, 1020, 1040, 320, 340) 
         --AND D.STAT IN (1000,300)
         AND M.NO = D.STAT   
         AND LOG.TIME >= VBDATE
         AND LOG.TIME <  VEDATE ;  
                
     -- 自营进退
