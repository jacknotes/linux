    BEGIN
    UPDATE H4RTMP_STKIN SET CTBREGED = 1
    where (num,CLS) in(select DTL.SRCNUM,DTL.SRCCLS from VdrChkRcv v, VdrChkRcvdtl dtl
                     where v.num = dtl.num and v.cls = dtl.cls and DTL.checked =1 and v.stat IN(100,300));
   -- COMMIT;  
    END;                                                      
    END;           
