    BEGIN
     -- 自营进
     INSERT INTO H4RTMP_STKIN(NUM,CLS,BILLTOCOE,BILLTONAME,VENDORCOE,VENDORNAME,DIRALCSTAT,LOGSTATE,STKINTOTAL,STKINTAX,
     STKINQSINTOTAL,STKBCKTOTAL,STKBCKTAX,STKBCKQSTOTAL,SUMSTKTOTAL,SUMSTKTAX,SUMSTKQSTOTAL,DIRINTOTAL,DIRINTAX,
