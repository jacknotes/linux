     PAYMODE,IVCREGED,IVCPAY,CTBREGED,TIME,FLAG)
     SELECT  D.NUM,
             D.CLS,
             VBILLTO.CODE ,
             VBILLTO.NAME ,
             VVENDOR.CODE ,
             VVENDOR.NAME ,
             M.STATNAME ,
             LOG.STAT ,
             D.TOTAL,
             D.TAX ,
             D.TOTAL - D.TAX ,
             0,
             0,
             0,
             D.TOTAL,
             D.TAX ,
