<VERSION>4.1.0</VERSION>
<SQLQUERY>
<REMARK>
[标题]

[应用背景]

[结果列描述]
   不找零
        在收银的过程中，有不少付款方式是不找零的。
	不找零的部分，代表的是门店收到的钱比销售金额多。
    销售金额=现金+购物券+会员卡+会员积分兑奖+转账支票+银联卡+移动积分+转账 - 在线充值
         因为在线充值是作为商品，记录入了付款方式的销售报表中，在统计的时候，要扣减这
部
分

    收入金额= 销售金额 + 在线充值 + 找零充值
        收入金额包含了门店的 代收费和 商品销售部分
[必要的查询条件]

[实现方法]

[其它]
exec hdcreatesynonym('H4RCALC_SYYRHZ_CALC_N1');
grant execute on H4RCALC_SYYRHZ_CALC_N1 to role_hdqry;
</REMARK>
<BEFORERUN>
declare
   piFromdate   date ;
   piToDate     date ;  
   VFLOWNO      varchar2(20); 
   vStoreCode   varchar2(20);
   piCls  varchar2(20);
   
begin
   

  piCls := '\(1,1)' ;
  --piFromdate := to_date('2009.06.01','yyyy.mm.dd') ;
  --piToDate   := to_date('2009.06.11','yyyy.mm.dd') ;      
  vStoreCode := '';

  piFromdate := to_date('\(2,1)','yyyy.mm.dd') ;
  piToDate   := to_date('\(3,1)','yyyy.mm.dd') ;

  H4RCALC_SYYRHZ_CALC_N1(piFromdate , piToDate ) ; --不计算跨期销售
   
  IF piCls = '门店合计_日期' THEN

   --按门店 合计一天的收银情况
   insert into H4RTMP_SYYRHJ (storecode , storename , cls , fildate ,
     amt0 , amt1 ,amt2 ,amt3 ,amt4 ,amt5 ,amt6 ,amt7 ,amt8 ,amt9 ,amt10 ,
     amt11,amt12,amt13,amt14,amt15,amt16,amt17,amt18,amt19,amt20 ,
     Cnt1 , cnt2 , Cnt3 ,cnt4,cnt5,cnt6,cnt7,cnt8,cnt9,cnt10 ,
     Cnt11 , cnt12 , Cnt13 ,cnt14,cnt15,cnt16,cnt17,cnt18,cnt19,cnt20 ,
     Piflag ,
     Orderby
     )
   select storecode , storename , piCls , trunc(fildate) ,
       sum(nvl(amt0,0)) amt0 ,--收银款
       sum(nvl(amt1,0)) amt1 , --现金有找零
       sum(nvl(amt2,0)) amt2 ,
       sum(nvl(amt3,0)) amt3 ,
       sum(nvl(amt4,0)) amt4 ,
       sum(nvl(amt5 ,0)) amt5 ,
       sum(nvl(amt6,0)) amt6 ,
       sum(nvl(amt7,0)) amt7 ,
       sum(nvl(amt8,0)) amt8 ,
       sum(nvl(amt9,0)) amt9 ,
       sum(nvl(amt10,0)) amt10 ,
       sum(nvl(amt11,0)) amt11 ,
       sum(nvl(amt12,0)) amt12 ,
       sum(nvl(amt13,0)) amt13 ,
       sum(nvl(amt14,0)) amt14 ,
       sum(nvl(amt15,0)) amt15 ,
       sum(nvl(amt16,0)) amt16 ,
       sum(nvl(amt17,0)) amt17 ,
       sum(nvl(amt18,0))  amt18 , --跨期销售的数据
       sum(nvl(amt19,0))  amt19 ,
       sum(nvl(amt20,0))   amt20 ,
       sum(nvl(Cnt1,0)) Cnt1 , --销售笔数
       sum(nvl(cnt2,0)) cnt2 , --退货、冲账笔数
       0 Cnt3 , 0 cnt4,sum(nvl(cnt5,0)) cnt5, 0 cnt6, 0 cnt7,0 cnt8, 0 cnt9,0 cnt10 ,
       0 Cnt11 , 0 Cnt12 ,
       0 Cnt13 , 0 cnt14, 0 cnt15, 0 cnt16, 0 cnt17,0 cnt18, 0 cnt19,0 cnt20 ,
       1 Piflag ,
       0 Orderby
     from H4RTMP_SYYRHJ
      where piFlag = 0
     group by storecode , storename , trunc(fildate);
    commit;

     DELETE FROM H4RTMP_SYYRHJ WHERE PIFLAG = 0 ;
     COMMIT;

  ELSIF piCls = '门店合计' THEN

   --按门店 合计一天的收银情况
   insert into H4RTMP_SYYRHJ (storecode , storename , cls , fildate ,
     amt0 , amt1 ,amt2 ,amt3 ,amt4 ,amt5 ,amt6 ,amt7 ,amt8 ,amt9 ,amt10 ,
     amt11,amt12,amt13,amt14,amt15,amt16,amt17,amt18,amt19,amt20 ,
     Cnt1 , cnt2 , Cnt3 ,cnt4,cnt5,cnt6,cnt7,cnt8,cnt9,cnt10 ,
     Cnt11 , cnt12 , Cnt13 ,cnt14,cnt15,cnt16,cnt17,cnt18,cnt19,cnt20 ,
     Piflag ,
     Orderby
     )
    select storecode , storename , piCls , piFromdate ,
       sum(nvl(amt0,0)) amt0 ,--收银款
       sum(nvl(amt1,0)) amt1 , --现金有找零
       sum(nvl(amt2,0)) amt2 ,
       sum(nvl(amt3,0)) amt3 ,
       sum(nvl(amt4,0)) amt4 ,
       sum(nvl(amt5 ,0)) amt5 ,
       sum(nvl(amt6,0)) amt6 ,
       sum(nvl(amt7,0)) amt7 ,
       sum(nvl(amt8,0)) amt8 ,
       sum(nvl(amt9,0)) amt9 ,
       sum(nvl(amt10,0)) amt10 ,
       sum(nvl(amt11,0)) amt11 ,
       sum(nvl(amt12,0)) amt12 ,
       sum(nvl(amt13,0)) amt13 ,
       sum(nvl(amt14,0)) amt14 ,
       sum(nvl(amt15,0)) amt15 ,
       sum(nvl(amt16,0)) amt16 ,
       sum(nvl(amt17,0)) amt17 ,
       sum(nvl(amt18,0))  amt18 , --跨期销售的数据
       sum(nvl(amt19,0))  amt19 ,
       sum(nvl(amt20,0))   amt20 ,
       sum(nvl(Cnt1,0)) Cnt1 , --销售笔数
       sum(nvl(cnt2,0)) cnt2 , --退货、冲账笔数
       0 Cnt3 , 0 cnt4, sum(nvl(cnt5,0)) cnt5, 0 cnt6, 0 cnt7,0 cnt8, 0 cnt9,0 cnt10 ,
       0 Cnt11 , 0 Cnt12 ,
       0 Cnt13 , 0 cnt14, 0 cnt15, 0 cnt16, 0 cnt17,0 cnt18, 0 cnt19,0 cnt20 ,
       1 Piflag ,
       --(case when max(fildate)-)
       (case when max(fildate)- min(fildate) + 1 <> trunc(least(sysdate+1,piToDate)) - min(fildate) then 1 else 0 end
       ) Orderby
     from H4RTMP_SYYRHJ
      where piFlag = 0
     group by storecode , storename ;
    commit;
    
     ---对于收银员合计中，如果门店收到钱的日期在 选择的时间段中有断的，要单独显示出来。
     ---- 修改 H4RTMP_SYYRHJ  的 Orderby 为 1 
     

     DELETE FROM H4RTMP_SYYRHJ WHERE PIFLAG = 0 ;
     COMMIT;     
     
  
  END IF;  

end;
</BEFORERUN>
