<VERSION>4.1.0</VERSION>
<SQLQUERY>
<REMARK>
[标题]

[应用背景]

[结果列描述]

[必要的查询条件]

[实现方法]

[其它]
</REMARK>
<BEFORERUN>
declare
vbegdate date;
venddate date;

begin
  vbegdate := to_date('\(1,1)','yyyy.mm.dd');
  venddate := to_date('\(2,1)','yyyy.mm.dd');

  delete from hdtmp_ClientAccount_JXCDRPT2;
  delete from hdtmp_ClientAccount_JXCDRPT;
  commit;

  --期初 
  insert into hdtmp_ClientAccount_JXCDRPT2(fildate,ClientKEY,QC,QCCHGBOOK,QCPrePay,qcyk,RegQC,qctax,qccamt,qcctax,QCCHGBOOKdeduct)
  select vbegdate,ClientKEY,QC,QCCHGBOOK,QCPrePay,qcyk,RegQC,qctax,qccamt,qcctax,QCCHGBOOKdeduct
  from RPT_ClientAccount_JXCDRPT
  where fildate = vbegdate ;

  --期末
  insert into hdtmp_ClientAccount_JXCDRPT2(fildate,ClientKEY,QM,QMCHGBOOK,QMPrePay,realqm,qMyk,RegQM,qmtax,qmcamt,qmctax,QMCHGBOOKdeduct)
  select vbegdate,ClientKEY,QM,QMCHGBOOK,QMPrePay,realqm,qMyk,RegQM,qmtax,qmcamt,qmctax,QMCHGBOOKdeduct
  from RPT_ClientAccount_JXCDRPT
  where fildate = venddate ;

  --发生  
  insert into hdtmp_ClientAccount_JXCDRPT2(fildate,ClientKEY,
                                           Purchasetotal,Returntotal,PayTotal,RegPurchasetotal,
                                           Purchasetax,Returntax,RegPurchasetax,
                                           Purchasecamt,Returncamt,RegPurchasecamt,
                                           Purchasectax,Returnctax,RegPurchasectax,
                                           CHGBOOKtotal,CHGBOOKpaytotal,CHGBOOKbcktotal,CHGBOOKusetotal,
                                           CHGBOOKdeducttotal,CHGBOOKdeductpaytotal,CHGBOOKbckdeducttotal,CHGBOOKdeductusetotal,
                                           PrePaytotal,PrePayrtntotal,UsePrePay,
                                           yktotal,ykbcktotal)
  select vbegdate,ClientKEY,
         sum(Purchasetotal),sum(Returntotal),sum(PayTotal),sum(RegPurchasetotal),
         sum(Purchasetax),sum(Returntax),sum(RegPurchasetax),
         sum(Purchasecamt),sum(Returncamt),sum(RegPurchasecamt),
         sum(Purchasectax),sum(Returnctax),sum(RegPurchasectax),
         sum(CHGBOOKtotal),sum(CHGBOOKpaytotal),sum(CHGBOOKbcktotal),sum(CHGBOOKusetotal),
         sum(CHGBOOKdeducttotal),sum(CHGBOOKdeductpaytotal),sum(CHGBOOKbckdeducttotal),sum(CHGBOOKdeductusetotal),
         sum(PrePaytotal),sum(PrePayrtntotal),sum(UsePrePay),
         sum(yktotal),sum(ykbcktotal)
  from RPT_ClientAccount_JXCDRPT
  where fildate between vbegdate and venddate
  group by ClientKEY;

  --汇总
  insert into hdtmp_ClientAccount_JXCDRPT( fildate,ClientKEY,
                                           QC,QCCHGBOOK,QCPrePay,qcyk,RegQC,qctax,qccamt,qcctax,QCCHGBOOKdeduct,
                                           QM,QMCHGBOOK,QMPrePay,realqm,qMyk,RegQm,qmtax,qmcamt,qmctax,QMCHGBOOKdeduct,
                                           Purchasetotal,Returntotal,PayTotal,RegPurchasetotal,
                                           Purchasetax,Returntax,RegPurchasetax,
                                           Purchasecamt,Returncamt,RegPurchasecamt,
                                           Purchasectax,Returnctax,RegPurchasectax,
                                           CHGBOOKtotal,CHGBOOKpaytotal,CHGBOOKbcktotal,CHGBOOKusetotal,
                                           CHGBOOKdeducttotal,CHGBOOKdeductpaytotal,CHGBOOKbckdeducttotal,CHGBOOKdeductusetotal,
                                           PrePaytotal,PrePayrtntotal,UsePrePay,
                                           yktotal,ykbcktotal)
  select fildate,ClientKEY,
         sum(QC),sum(QCCHGBOOK),sum(QCPrePay),sum(qcyk),sum(RegQC),sum(qctax),sum(qccamt),sum(qcctax),sum(QCCHGBOOKdeduct),
         sum(QM),sum(QMCHGBOOK),sum(QMPrePay),sum(realqm),sum(qMyk),sum(RegQM),sum(qmtax),sum(qmcamt),sum(qmctax),sum(QMCHGBOOKdeduct),
         sum(Purchasetotal),sum(Returntotal),sum(PayTotal),sum(RegPurchasetotal),
         sum(Purchasetax),sum(Returntax),sum(RegPurchasetax),
         sum(Purchasecamt),sum(Returncamt),sum(RegPurchasecamt),
         sum(Purchasectax),sum(Returnctax),sum(RegPurchasectax),
         sum(CHGBOOKtotal),sum(CHGBOOKpaytotal),sum(CHGBOOKbcktotal),sum(CHGBOOKusetotal),
         sum(CHGBOOKdeducttotal),sum(CHGBOOKdeductpaytotal),sum(CHGBOOKbckdeducttotal),sum(CHGBOOKdeductusetotal),
         sum(PrePaytotal),sum(PrePayrtntotal),sum(UsePrePay),
         sum(yktotal),sum(ykbcktotal)
  from hdtmp_ClientAccount_JXCDRPT2
  group by fildate,ClientKEY;

  commit;
end;
</BEFORERUN>
