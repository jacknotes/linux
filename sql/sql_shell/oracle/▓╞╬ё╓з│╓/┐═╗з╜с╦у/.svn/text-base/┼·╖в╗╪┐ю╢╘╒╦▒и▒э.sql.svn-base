<BEFORERUN>
declare
vbegin date;
vend   date;
begin
  vbegin := '\(1,1)';
  vend   := '\(2,1)';
  
  insert into H4TMP_SALDRPT_COUNT(num,cls,Clientcode,Clientname,Stat,TOTAL,Note,billcode,billname,wrh,time)
  select a.num,a.cls,d.code,d.name,a.stat,a.total,a.note,c.code,c.name,w.gid,vbegin
  from stkout a,stkoutlog log ,clienth c,clienth d,warehouse w
  where a.num = log.num
  and a.cls = log.cls
  and a.cls = '批发'
  and a.billto = c.gid
  and a.client = d.gid
  and log.stat in (700,720,740,320,340)
  and a.wrh = w.gid
  and log.time >=  vbegin
  and log.time < vend;
  
  insert into H4TMP_SALDRPT_COUNT(num,cls,Clientcode,Clientname,Stat,TOTAL,Note,billcode,billname,wrh,time)
  select a.num,a.cls,d.code,d.name,a.stat,-a.total,a.note,c.code,c.name,w.gid ,vbegin
  from stkoutbck a,stkoutbcklog log,clienth c,clienth d,warehouse w
  where a.num = log.num
  and a.cls = log.cls
  and a.cls = '批发退'
  and a.billto = c.gid
  and a.client = d.gid
  and log.stat in (1000,1020,1040,320,340)
  and a.wrh = w.gid
  and log.time >=  vbegin
  and log.time < vend; 
  
  insert into H4TMP_SALDRPT_COUNT(num,cls,Clientcode,Clientname,Stat,TOTAL,Note,billcode,billname,wrh,time)   
  select a.num,a.cls,d.code,d.name,a.stat,a.total,a.note,c.code,c.name,w.gid ,vbegin 
  from alcdiff a,alcdifflog log,clienth c,clienth d,warehouse w,stkout mst
  where a.num = log.num
  and a.cls = log.cls
  and a.cls = '批发差异'
  and a.billto = c.gid
  and a.store = d.gid
  and log.stat = 100 
  and mst.wrh = w.gid
  and mst.num = a.srcnum and mst.cls = '批发'
  and log.time >=  vbegin
  and log.time < vend;
  
  insert into H4TMP_SALDRPT_COUNT(num,cls,Clientcode,Clientname,Stat,TOTAL,Note,billcode,billname,wrh,time)
  select a.num,a.cls,d.code,d.name,a.stat,a.ototal-a.itotal,a.note,c.code,c.name,'',vbegin
  from wholesaleexg a,wholesaleexglog log,clienth c,clienth d
  where a.num = log.num
  and a.cls = log.cls
  and a.cls = '批发换货'
  and a.billto = c.gid
  and a.client = d.gid
  and log.stat = 100
  and log.time >= vbegin
  and log.time < vend; 
  ---------更新对账单信息
    update H4TMP_SALDRPT_COUNT a set chknum = (select max(b.num) from ClientChkRcvdtl b,clientchkrcv c
where b.srccls = a.cls and b.srcnum = a.num and b.num = c.num and c.stat in(100,300))
  where (a.num,a.cls) in (select srcnum,srccls from ClientChkRcvdtl);
  update H4TMP_SALDRPT_COUNT a set chkstat = (select stat from ClientChkRcv 
where num = a.chknum)
  where a.chknum in (select num from ClientChkRcv);
  ---------更新发票信息
  update H4TMP_SALDRPT_COUNT a set svinum = (select distinct b.num from svidtl b,svi c
where b.srccls = a.cls and b.srcnum = a.num and b.num = c.num and c.stat in (600) )
  where (a.num,a.cls) in (select srcnum,srccls from svidtl);
  update H4TMP_SALDRPT_COUNT a set svistat = (select stat from svi where num = 
 a.svinum)
  where a.svinum in (select num from svi);
  ---------更新收款登记信息,走对账
  update H4TMP_SALDRPT_COUNT a set cpaynum = (select max(b.num) from 
cpaychkrcvdtl b,cpay c where chkrcvnum = a.chknum and b.num = c.num and c.stat in (0,1700,500) and a.num = b.srcnum and a.cls = b.srccls )
  where a.chknum in (select chkrcvnum from cpaychkrcvdtl);
  ---------更新收款登记信息,走发票
  update H4TMP_SALDRPT_COUNT a set cpaynum = (select max(b.num) from cpaydtl b,cpay c 
where b.svinum = a.svinum and b.num = c.num and c.stat in (0,1700,500))
  where a.svinum in (select svinum from cpaydtl);
  ---------更新收款登记状态
  update H4TMP_SALDRPT_COUNT a set cpaystat = (select stat from cpay where num 
= a.cpaynum)
  where a.cpaynum in (select num from cpay);
end;
</BEFORERUN>
