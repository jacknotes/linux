<VERSION>4.1.1</VERSION>
<SQLQUERY>
<REMARK>
[标题]
门店商品查询
[应用背景]

[结果列描述]

[必要的查询条件]
必须填写【门店代码】或者【商品代码】
[实现方法]

[临时表结构]
/********增加临时表********/

CREATE TABLE h4rtmp_STOREGOODS
AS
SELECT  unique (STORE.CODE) code, STORE.NAME NAME, 
GOODS.CODE CODE1, GOODS.NAME NAME1, 
GDSTORE.INVHIGHBOUND INVHIGHBOUND, GDSTORE.INVLOWBOUND 
INVLOWBOUND, GDSTORE.HIGHINV HIGHINV, 
GDSTORE.LOWINV LOWINV, DECODE(GDSTORE.ISLTD,0,'不限制',1,'限制') 
ISLTD, DECODE(GDSTORE.AUTOALOC,0,'是',1,'否') AUTOALOC, 
DECODE(GDSTORE.ALWSORD,0,'否',1,'一次叫货',2,'二次叫货',3,'三次叫
货') ALWSORD, 
GDSTORE.STDSHOW STDSHOW, (select name from limitqty where no = 
GDSTORE.ALLOCLIMITQTY and LIMITQTY.CLS = '配货') ALLOCLIMITQTY, 
(select name from Supplyqty where no = GDSTORE.ALLOCSUPPLYQTY 
and SUPPLYQTY.CLS = '配货') ALLOCSUPPLYQTY, 
GDSTORE.DISP DISP
FROM GDSTORE GDSTORE, GOODS GOODS, STORE STORE, LIMITQTY 
LIMITQTY, SUPPLYQTY SUPPLYQTY
WHERE ((GDSTORE.STOREGID = STORE.GID)
 and  (GDSTORE.GDGID = GOODS.GID)
 and  (SUPPLYQTY.CLS = '配货')
 and  (LIMITQTY.CLS = '配货') )
ORDER BY code ASC, CODE1 ASC
;
EXEC HDCREATESYNONYM('h4rtmp_STOREGOODS');
/
[版本记录]
20100528/ 
20101002/LINING
[其它]
</REMARK>
<BEFORERUN>
declare
  vstorecode varchar2(100);
  vgdcode varchar2(100);
begin

  vstorecode := trim('\(1,1)');
  vgdcode  := trim('\(2,1)');

  delete h4rtmp_STOREGOODS;
  commit;
  insert into h4rtmp_STOREGOODS
    (code,
     Name,
     Code1,
     Name1,
     Invhighbound,
     Invlowbound,
     Highinv,
     Lowinv,
     Isltd,
     Autoaloc,
     Alwsord,
     Stdshow,
     Alloclimitqty,
     Allocsupplyqty,
     Disp,
     inprc,
     flag,
     lifecycle,
     invqty,
     isduqpc)
    SELECT 
     unique(STORE.CODE) code,
     STORE.NAME NAME,
     GOODS.CODE CODE1,
     GOODS.NAME NAME1,
     GDSTORE.INVHIGHBOUND INVHIGHBOUND,
     GDSTORE.INVLOWBOUND INVLOWBOUND,
     GDSTORE.HIGHINV HIGHINV,
     GDSTORE.LOWINV LOWINV,
     DECODE(GDSTORE.ISLTD, 0, '不限制', 1, '限制') ISLTD,
     DECODE(GDSTORE.AUTOALOC, 0, '是', 1, '否') AUTOALOC,
     DECODE(GDSTORE.ALWSORD,
             0,
             '否',
             '是')
/*             '一次叫货',
             2,
             '二次叫货',
             3,
             '三次叫货') */
ALWSORD,
     GDSTORE.STDSHOW STDSHOW,
     (select name
        from limitqty
       where no = GDSTORE.ALLOCLIMITQTY
         and LIMITQTY.CLS = '配货') ALLOCLIMITQTY,
     (select name
        from Supplyqty
       where no = GDSTORE.ALLOCSUPPLYQTY
         and SUPPLYQTY.CLS = '配货') ALLOCSUPPLYQTY,
     GDSTORE.DISP DISP,
     nvl(gdstore.inprc,0),
     (select count(1) from gdsaleschemedtl where code = nvl(store.license,store.gdsalescheme) and 
        gdgid = goods.gid),
      goods.lifecycle,
      nvl(bs.qty,0),
      gdqpc.qpc
      FROM GDSTORE   GDSTORE,
           GOODS     GOODS,
           STORE     STORE,
           LIMITQTY  LIMITQTY,
           SUPPLYQTY SUPPLYQTY,
           businvs bs,
           (select gid,qpc from gdqpc where isdu = 2) gdqpc
     WHERE (GDSTORE.STOREGID = STORE.GID) and (GDSTORE.GDGID = GOODS.GID) and goods.gid = gdqpc.gid and 
           (SUPPLYQTY.CLS = '配货') and (LIMITQTY.CLS = '配货') and gdstore.storegid = bs.store(+) and gdstore.gdgid = bs.gdgid(+)
       and (store.code = vstorecode or vstorecode  is null)
       and (goods.code = vgdcode or vgdcode is null)       
     ORDER BY code ASC, CODE1 ASC;
   commit;
end;
</BEFORERUN>
